	<queryString>
		<![CDATA[SELECT
     pedido.`idPedido` AS Codigo_do_Pedido,
     pedido.`data_criacao` AS Data_Criação,
     pedido.`status` AS Status_do_Pedido,
     pedido.`valor_total` AS Valor_Total,
     funcionario.`Nome` AS Nome_Funcionario
FROM
     `funcionario` funcionario INNER JOIN `pedido` pedido ON funcionario.`idFuncionario` = pedido.`funcionario`
WHERE
     pedido.`status` = "FECHADO"
 AND pedido.`data_criacao` BETWEEN $P{data_inicio} and $P{data_fim}
ORDER BY
     pedido.`data_criacao` ASC]]>
	</queryString>
