	<queryString>
		<![CDATA[SELECT
     produto.`descricao` AS produto_descricao,
     produto.`quantidadeMinima` AS produto_quantidadeMinima,
     produto.`quantidade_estoque` AS produto_quantidade_estoque,
     categoria.`descricao` AS categoria_descricao,
     produto.`dataValidade` AS produto_dataValidade
FROM
     `categoria` categoria INNER JOIN `produto` produto ON categoria.`id` = produto.`categoria_id`
WHERE
     produto.`quantidade_estoque` <= produto.`quantidadeMinima`
ORDER BY
     produto.`quantidadeMinima` ASC]]>
	</queryString>
